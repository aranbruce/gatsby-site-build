{
  "name": "aranbc",
  "description": "AranBC Personal Website",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env.development  gatsby develop --https --open",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aranbruce/gatsby-site-build"
  },
  "author": "Aran Bruce-Caddick",
  "bugs": {
    "url": "https://github.com/aranbruce/gatsby-site-build/issues"
  },
  "lighthouse": {
    "requiredScores": {
      "performance": 90,
      "accessibility": 90,
      "best-practices": 80,
      "seo": 80
    }
  },
  "dependencies": {
    "gatsby": "^2.8.6",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.0.21",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-layout": "^1.0.15",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-source-filesystem": "^2.0.38",
    "gatsby-transformer-remark": "^2.3.12",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-svg": "^10.0.6",
    "react-transition-group": "^4.1.1",
    "smooth-scroll": "^16.0.3"
  },
  "devDependencies": {
    "env-cmd": "^9.0.3",
    "prettier": "^1.18.2"
  }
}
